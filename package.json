    "babel-runtime": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "rimraf": "^2.6.1",
    "terser-webpack-plugin": "^1.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
